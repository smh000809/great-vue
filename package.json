    "eslint-plugin-vue": "^9.18.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.1.0",
    "npm-run-all2": "^6.1.1",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "start-server-and-test": "^2.0.2",
    "terser": "^5.24.0",
    "typescript": "~5.2.2",
    "unocss": "^0.57.4",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-vue-components": "^0.25.2",
