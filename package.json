    "@types/jsdom": "^21.1.5",
    "@types/lodash-es": "^4.17.11",
    "@types/node": "^20.9.0",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-config": "^0.57.4",
    "@unocss/reset": "^0.57.4",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitejs/plugin-vue": "^4.4.1",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vitest/ui": "^0.34.6",
