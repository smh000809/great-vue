{
  // 将命令启动器与窗口标题一起显示。
  "window.commandCenter": true,
  // 关闭 less、scss、css 语法错误提示
  "less.lint.unknownAtRules": "ignore",
  "scss.lint.unknownAtRules": "ignore",
  "css.lint.unknownAtRules": "ignore",
  // 开启点击参数提示快速跳转到参数声明
  "editor.inlayHints.enabled": "on",
  "javascript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterNames.enabled": "all",
  // 控制每个方括号类型是否具有自己的独立颜色池。
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  // 在保存时自动修复代码
  "editor.codeActionsOnSave": {
    "source.fixAll": false,
    "source.fixAll.eslint": true
  },
  // 控制列表和树是否具有平滑滚动效果。
  "workbench.list.smoothScrolling": true,
  // material-icon 插件配置
  "material-icon-theme.activeIconPack": "vue",
  // 控制命令面板是否应包含常用命令的列表。
  "workbench.commandPalette.experimental.suggestCommands": true,
  // 指定工作台中使用的文件图标主题；若指定为 "null"，则不显示任何文件图标。
  "workbench.iconTheme": "material-icon-theme",
  // 控制是否启用平滑插入动画。
  "editor.cursorSmoothCaretAnimation": "on",
  // 控制字体
  "editor.fontFamily": "'Monaspace Neon', Menlo, Monaco, 'Courier New', monospace",
  // 字体连字
  "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08', 'calt', 'dlig'",
  // 控制编辑器是否自动格式化粘贴的内容。格式化程序必须可用，并且能针对文档中的某一范围进行格式化。
  "editor.formatOnPaste": true,
  // 控制编辑器是否已启用链接编辑。相关符号(如 HTML 标记)将在编辑时进行更新，具体取决于语言。
  "editor.linkedEditing": true,
  // 控制编辑器是否使用动画滚动。
  "editor.smoothScrolling": true,
  // 在编辑器顶部的滚动过程中显示嵌套的当前作用域。
  "editor.stickyScroll.enabled": true,
  // 定义要显示的最大粘滞行数。
  "editor.stickyScroll.maxLineCount": 5,
  // 控制单击已折叠的行后面的空内容是否会展开该行。
  "editor.unfoldOnClickAfterEndOfLine": true,
